{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "actb/laravel-form-helpers": "^1.6",
        "actb/laravel-validation-rules": "^1.2",
        "blade-ui-kit/blade-heroicons": "^0.2.1",
        "cybercog/laravel-ban": "^4.2",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "laravolt/avatar": "^4.0",
        "pragmarx/google2fa-laravel": "^1.4",
        "spatie/data-transfer-object": "^2.3",
        "spatie/laravel-activitylog": "^3.14",
        "spatie/laravel-enum": "^1.6",
        "spatie/laravel-flash": "^1.6",
        "spatie/laravel-permission": "^3.16",
        "spatie/laravel-queueable-action": "^2.6",
        "spatie/laravel-schedule-monitor": "^1.0",
        "watson/active": "^6.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fakerphp/faker": "^1.9.1",
        "jasonmccreary/laravel-test-assertions": "^1.0",
        "laravel/telescope": "^4.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",

        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
